---
// src/components/dashboard/SettingsView.astro
export interface Props {
  workspaceId: string;
}
const { workspaceId } = Astro.props;
---
<div>
  <h1 class="text-2xl font-semibold text-gray-800 mb-8">
    Configuración del Workspace
